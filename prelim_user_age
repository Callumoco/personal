-- For visualisation showing range of ages
SELECT 
age,
COUNT(user_id) AS count_users

FROM `analytics-take-home-test.monzo_product.users` 

GROUP BY 1


-- For determining the range of ages to group for tracking
SELECT 
PERCENTILE_CONT(age, 0.25) OVER() AS lower_quartile,
PERCENTILE_CONT(age, 0.5) OVER() AS median,
PERCENTILE_CONT(age, 0.75) OVER() AS upper_quartile,

FROM `analytics-take-home-test.monzo_product.users` 
